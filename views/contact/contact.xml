<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="view_contact_form_inherit" model="ir.ui.view">
      <field name="name">res.partner.external</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="arch" type="xml">
          <xpath expr="//field[@name='type']" position="before">
                <field name="father_name"  options="{'no_create_edit': True, 'no_quick_create': True,'no_open': True}"/>
                <field name="mother_name"  options="{'no_create_edit': True, 'no_quick_create': True,'no_open': True}"/>
          </xpath>
          <xpath expr="//field[@name='type']" position="after">
                <field name="num_colab"  options="{'no_create_edit': True, 'no_quick_create': True,'no_open': True}"/>
                <field name="date"  options="{'no_create_edit': True, 'no_quick_create': True,'no_open': True}"/>

                <field name="payroll"  options="{'no_create_edit': True, 'no_quick_create': True,'no_open': True}"/>
                <field name="phone"  options="{'no_create_edit': True, 'no_quick_create': True,'no_open': True}"/>
                <field name="email"  options="{'no_create_edit': True, 'no_quick_create': True,'no_open': True}"/>
          </xpath>
      </field>
    </record>

  </data>
</odoo>
 